      ],
      "implements": [],
      "implemented_by": [],
      "note": "LifeCycleInfo describes the life cycle state of an element together with additional information like what to use instead",
      "sources": [
        {
          "pdf_file": "AUTOSAR_FO_TPS_GenericStructureTemplate.pdf",
          "page_number": 392,
          "autosar_standard": "Foundation",
          "standard_release": "R23-11"
        },
        {
          "pdf_file": "AUTOSAR_FO_TPS_StandardizationTemplate.pdf",
          "page_number": 195,
          "autosar_standard": "Foundation",
          "standard_release": "R23-11"
        }
      ],
      "attributes": {
        "lcObject": {
          "type": "Referrable",
          "multiplicity": "1",
          "kind": "reference",
          "is_ref": true,
          "note": "Element(s) have the life cycle as described in lcState."
        },
        "lcState": {
          "type": "LifeCycleState",
          "multiplicity": "0..1",
          "kind": "reference",
          "is_ref": false,
          "note": "This denotes the particular state assigned to the object. If is given then the default life cycle state of Life assumed."
        },
        "periodBegin": {
          "type": "LifeCyclePeriod",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Starting point of period in which the element has the cycle state lcState. If no periodBegin is given default period begin of LifeCycleInfoSet is 535 Document ID 202: AUTOSAR_FO_TPS_GenericStructureTemplate Template R23-11"
        },
        "periodEnd": {
          "type": "LifeCyclePeriod",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Expiry date, i.e. end point of period the element does not denoted life cycle state lcState any more. If no given then the default period begin of Life assumed."
        },
        "remark": {
          "type": "DocumentationBlock",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Remark describing for example the element was given the specified life cycle semantics of useInstead"
        },
        "useInstead": {
          "type": "Referrable",
          "multiplicity": "*",
          "kind": "reference",
          "is_ref": true,
          "note": "Element(s) that should be used instead of the one referrable. in case of life cycle states lcState unlike case there are multiple references the exact be individually described in the remark."
        }
      }
    }
