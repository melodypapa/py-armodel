          "standard_release": "R23-11"
        }
      ],
      "attributes": {
        "arRelease": {
          "type": "RevisionLabelString",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Version of the AUTOSAR Release the element referred to part of. contains three levels (major, minor, are defined by AUTOSAR."
        },
        "date": {
          "type": "DateTime",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Date within period."
        },
        "productRelease": {
          "type": "RevisionLabelString",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "Version of the product within the period."
        }
      }
