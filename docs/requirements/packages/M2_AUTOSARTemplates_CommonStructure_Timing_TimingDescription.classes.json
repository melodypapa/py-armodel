        "clockReference": {
          "type": "TimingClock",
          "multiplicity": "0..1",
          "kind": "reference",
          "is_ref": false,
          "note": "Optional reference to a clock that holds the time base for event."
        },
        "occurrence": {
          "type": "TDEventOccurrence",
          "multiplicity": "0..1",
          "kind": "attribute",
          "is_ref": false,
          "note": "The occurrence expression for this event."
        }
